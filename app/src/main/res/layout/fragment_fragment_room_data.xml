<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              tools:context=".view.FragmentRoomData"
              android:orientation="vertical" android:padding="@dimen/activity_horizontal_margin">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <TextView
                android:text="@string/date_creation"
                android:layout_width="0dp"
                android:layout_height="wrap_content" android:id="@+id/textView13" android:layout_weight="1"
                android:textAlignment="center" android:textStyle="bold" android:fontFamily="@font/oxygen"/>
        <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content" android:id="@+id/date" android:layout_weight="1"
                android:textAlignment="center" android:fontFamily="@font/oxygen"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <TextView
                android:text="@string/number_user"
                android:layout_width="0dp"
                android:layout_height="wrap_content" android:id="@+id/textView14" android:layout_weight="1"
                android:textAlignment="center" android:textStyle="bold" android:fontFamily="@font/oxygen"/>
        <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content" android:id="@+id/numberOFUser" android:layout_weight="1"
                android:textAlignment="center" android:fontFamily="@font/oxygen"/>
    </LinearLayout>

</LinearLayout>